<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.viewproject.GameWindowController">

    <Label fx:id="difficultyText"/>

    <fx:define>
        <Group fx:id="sudokuBoard"/>
    </fx:define>

    <GridPane fx:id="sudokuBoardGrid">
    </GridPane>

    <HBox alignment="CENTER" spacing="20.0">
        <Button fx:id="saveBoardBtn" alignment="CENTER"/>
        <Button fx:id="loadBoardBtn" alignment="CENTER"/>
        <Button fx:id="checkBtn" alignment="CENTER"/>
    </HBox>

</VBox>