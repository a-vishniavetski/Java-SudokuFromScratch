<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.viewproject.NewGameController" fx:id="newGameVbox">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <fx:define>
        <ToggleGroup fx:id="langToggleGroup"/>
    </fx:define>

    <HBox fx:id="langRadiosHbox" alignment="TOP_CENTER" spacing="20.0">

    </HBox>

    <Label fx:id="welcomeText" text="%welcomeText"/>
    <Label fx:id="authorsText"/>
    <Label fx:id="difficultyText" text="%difficultyText"/>


    <fx:define>
        <ToggleGroup fx:id="difficultyToggleGroup"/>
    </fx:define>

    <VBox fx:id="radiosVbox" alignment="CENTER" spacing="20.0">

    </VBox>

    <Button fx:id="newGameBtn" text="%newGameBtn" onAction="#onStartGameBtnClick"/>
</VBox>
